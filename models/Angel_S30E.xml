      <pos x="0.80" y="0.00" z="-0.07" />
      <spring constant="100" damping="1" max_force="5" />
    </wheel>
    <!-- fuselage top -->
    <wheel percent_brake="1" caster_angle_rad="0">
      <pos x="-0.24" y="0.00" z="-0.43" />
      <spring constant="100" damping="1" max_force="5" />
    </wheel>
    <!-- rudder -->
    <wheel percent_brake="1" caster_angle_rad="0">
      <pos x="-3.04" y="0.00" z="-0.59" />
      <spring constant="100" damping="1" max_force="5" />
    </wheel>
    <!-- right elevator -->
    <wheel percent_brake="1" caster_angle_rad="0">
      <pos x="-2.94" y="0.94" z="0.0" />
      <spring constant="100" damping="1" max_force="5" />
    </wheel>
    <!-- left elevator -->
